<h1>Editing task</h1>

<% form_for :task, :url => task_list_task_path(:task_list_id => @task.task_list.id, :id => @task.id), :html => {:method => :put} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => f %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', task_list_task_path(:task_list_id => @task.task_list.id, :id => @task.id) %> |
<%= link_to 'Back', task_list_path(@task.task_list.id) %>